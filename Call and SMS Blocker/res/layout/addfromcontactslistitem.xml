<?xml version="1.0" encoding="utf-8"?>
<com.blocker.callandsms.addfromcontacts.AddFromContactListListItem
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  
  <TextView
  	android:id="@+id/addfromcontactlistitem_tName"
  	android:layout_height="wrap_content"
  	android:layout_width="fill_parent"
  	android:textSize="20sp"
  />
  <TextView
  	android:id="@+id/addfromcontactlistitem_tNumber"
  	android:layout_height="wrap_content"
  	android:layout_width="fill_parent"
  	android:layout_below="@+id/addfromcontactlistitem_tName"
  	android:textSize="20sp"
  />
</com.blocker.callandsms.addfromcontacts.AddFromContactListListItem>
